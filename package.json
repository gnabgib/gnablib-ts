{
	"name": "gnablib",
	"version": "0.2.3",
	"description": "A library for building web/node/deno applications",
	"types": "/release/index.d.ts",
	"scripts": {
		"preversion": "npm run deeptest",
		"version": "npm run build && git add -A",
		"postversion": "git push && git push --tags",
		"format": "prettier --write \"src/**/*.{ts,js}\"",
		"format:check": "prettier --check \"src/**/*.{js,ts}\"",
		"lint": "eslint \"src/**/*.{js,ts}\"",
		"test": "uvu -r tsm -i \"(.slow.test.ts|.log)$\" tst",
		"test:coverage": "c8 --include=src npm test",
		"test:report": "c8 report --reporter=html",
		"deeptest": "uvu -r tsm -i .log$ tst",
		"prebuild": "rm -rf dist && rm -rf release",
		"build": "tsc --sourceMap false",
		"compress": "compress",
		"doc": "typedoc"
	},
	"engines": {
		"node": ">=15.3.0"
	},
	"exports": {
		".": {
			"types": "./release/*.d.ts",
			"default": "./release/*.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"release"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gnabgib/gnablib-ts.git"
	},
	"keywords": [
		"ABNF",
		"Adler",
		"BCC",
		"cksum",
